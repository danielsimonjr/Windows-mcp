    "deepthinking-mcp": {
      "type": "stdio",
      "command": "node",
      "args": [
        "c:/mcp-servers/deepthinking-mcp/dist/index.js"
      ],
      "env": {}
    },
    "windows-mcp": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "--directory",
        "C:/mcp-servers/Windows-MCP",
        "run",
        "main.py"
      ],
      "env": {}
    }
  }
}
